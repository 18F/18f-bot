{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-console": 0,
    "prefer-destructuring": [0]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["src/test/**/*.js", "**/*.test.js"],
      "env": {
        "mocha": true
      }
    },
    { "files": ["src/**/*.test.js"], "env": { "jest": true } }
  ]
}
